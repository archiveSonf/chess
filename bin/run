#!/bin/bash
. ./bin/select.sh

printf "Souhaitez-vous lancer la version Windows ou Linux ?\n(en fonction de votre syst√®me) : "
bool Windows Linux
value=$(< bin/res/select.value)
if [ "$value" = "Windows" ]
then
  chmod +x ./dist/chesswn.exe
  ./dist/chesswn.exe
else
  chmod +x ./dist/chess.exe
  ./dist/chess.exe
fi